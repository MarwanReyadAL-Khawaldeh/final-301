<!DOCTYPE html>
<html>
<head>
    <meta charset='utf-8'>
    <meta http-equiv='X-UA-Compatible' content='IE=edge'>
    <link rel="stylesheet" href="../../css/reset.css">
<link rel="stylesheet" href="../../css/base.css">
<link rel="stylesheet" href="../../css/layout.css">
    <title>Product by price</title>
</head>
<body>
    <header>
        <nav>
            <a href="/">Home </a>
            <a href="/maybellineProducts">All Product </a>
            <a href="/addProduct">My Product </a>
        </nav>

    </header>
    <main>

        <div class="card">
            <div class="myCard">
                <% data.forEach(val =>{%>

                    <h2><%= val.name%></h2>
                    <img src="<%= val.img%>" alt="">
                    <h3><%= val.price%></h3>
                    <h3><%= val.description%></h3>

               <% }) %>
            </div>
        </div>
    </main>
    <footer>
        <h3>
            All Rights Reserved 2021 &#174
        </h3>
    </footer>
    
</body>
</html>